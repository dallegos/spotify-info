{
    "name": "spotify-info",
    "version": "1.0.0",
    "description": "A set of script that allows me to display what I'm currently playing on Spotify",
    "main": "app.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/app.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dallegos/spotify-info.git"
    },
    "keywords": [
        "javascript",
        "spotify",
        "typescript",
        "experimental",
        "spotify-api",
        "expressjs",
        "now-playing"
    ],
    "author": "David Gallegos (https://github.com/dallegos)",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/dallegos/spotify-info/issues"
    },
    "homepage": "https://github.com/dallegos/spotify-info#readme",
    "dependencies": {
        "dotenv": "^16.3.1",
        "etag": "^1.8.1",
        "express": "^4.18.2",
        "node-fetch": "2.6.6"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.0",
        "@types/node-fetch": "^2.6.4",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.44.0",
        "nodemon": "^2.0.22",
        "typescript": "^5.1.6"
    }
}
